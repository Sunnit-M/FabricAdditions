{
	"schemaVersion": 1,
	"id": "fabric-addition",
	"version": "${version}",
	"name": "Fabric Addition",
	"description": "This is an mod intended to add additions to minecraft through fabric",
	"authors": [
		"Sunnit"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/fabric-additions/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.sunnit.fabricaddition.FabricAdditions"
		],
		"client": [
			"net.sunnit.fabricaddition.FabricAdditionsClient"
		],
		"fabric-datagen": [
			"net.sunnit.fabricaddition.FabricAdditionsDataGenerator"
		]
	},
	"mixins": [
		"fabric-addition.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.22",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}